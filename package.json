    "fluidbox": "^2.0.5",
    "font-awesome": "^4.7.0",
    "gentelella": "^1.4.0",
    "jasny-bootstrap": "^3.1.3",
    "jquery": "^3.4",
    "node-sass": "^4.11.0",
    "pace": "github:hubspot/pace#v1.0.2",
