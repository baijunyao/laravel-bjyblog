{
    "name": "baijunyao/laravel-bjyblog",
    "description": "Laravel blog.",
    "keywords": ["framework", "laravel", "bjyblog", "blog"],
    "authors": [
        {
            "name": "baijunyao",
            "email": "baijunyao@baijunyao.com",
            "homepage": "https://baijunyao.com",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2",
        "apollopy/flysystem-aliyun-oss": "1.2.0",
        "appstract/laravel-opcache": "3.0.1",
        "baijunyao/laravel-flash": "2.0.0",
        "baijunyao/laravel-plugin-manager": "3.0.1",
        "baijunyao/laravel-print": "3.0.6",
        "baijunyao/laravel-restful": "1.1.0",
        "baijunyao/laravel-scout-elasticsearch": "3.0.1",
        "baijunyao/laravel-test-support": "4.0.0",
        "baijunyao/laravel-upload": "2.0.0",
        "composer/semver": "1.5.0",
        "doctrine/dbal": "2.9.2",
        "fideloper/proxy": "4.2.1",
        "fukuball/jieba-php": "0.33",
        "genealabs/laravel-model-caching": "0.7.0",
        "graham-campbell/markdown": "11.1.0",
        "intervention/image": "2.5.0",
        "jenssegers/agent": "2.6.3",
        "laravel/framework": "6.0.4",
        "laravel/passport": "7.5.0",
        "laravel/socialite": "4.2.0",
        "laravel/tinker": "1.0.10",
        "laravelium/feed": "6.0.1",
        "league/commonmark-ext-table": "2.1.0",
        "league/html-to-markdown": "4.8.2",
        "mews/purifier": "3.1.0",
        "overtrue/laravel-lang": "3.0.17",
        "predis/predis": "1.1.1",
        "sentry/sentry-laravel": "1.3.0",
        "socialiteproviders/qq": "3.0.1",
        "socialiteproviders/vkontakte": "4.0.0",
        "socialiteproviders/weibo": "3.0.0",
        "spatie/laravel-backup": "6.4.2",
        "spatie/laravel-sitemap": "5.5.0",
        "stichoza/google-translate-php": "4.0.2"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "2.6.5",
        "beyondcode/laravel-dump-server": "1.3.0",
        "filp/whoops": "2.5.0",
        "friendsofphp/php-cs-fixer": "2.15.3",
        "facade/ignition": "1.9.1",
        "fzaninotto/faker": "1.8.0",
        "itsgoingd/clockwork": "4.0.11",
        "laravel/dusk": "5.5.2",
        "mockery/mockery": "1.2.4",
        "nunomaduro/collision": "3.0.1",
        "phpunit/phpunit": "8.0.6"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "itsgoingd/clockwork"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "phpcs": "vendor/bin/php-cs-fixer fix",
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ]
    }
}
